  "next-unused": {
    "alias": {
      "@/app": "src/app/",
      "@/components": "src/components/",
      "@/constants": "src/constants/",
      "@/context": "src/context/",
      "@/styles": "src/styles",
      "@/themes": "src/themes",
      "@/types": "src/types/",
      "@/utils": "src/utils/",
      "src/app": "src/app/",
      "src/components": "src/components/",
      "src/constants": "src/constants/",
      "src/context": "src/context/",
      "src/styles": "src/styles",
      "src/themes": "src/themes",
      "src/types": "src/types/",
      "src/utils": "src/utils/",
      "src": "./src/"
    },
    "include": [
      "src/app",
      "src/components",
      "src/constants",
      "src/context",
      "src/styles",
      "src/themes",
      "src/types",
      "src/utils"
    ],
    "exclude": [],
    "entrypoints": [
      "src/app",
      "src/components",
      "src/constants",
      "src/context",
      "src/styles",
      "src/themes",
      "src/types",
      "src/utils"
    ],
    "extensions": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx",
      ".module.css",
      ".test.js",
      ".test.jsx",
      ".test.ts",
      ".test.tsx"
    ],
    "resolve": {
      "extensions": [
        ".ts",
        ".tsx",
        ".js",
        ".jsx"
      ],
      "mainFiles": [
        "index"
      ]
    }
  }
}
